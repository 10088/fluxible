    "lint": "eslint *.js",
    "postinstall": "gulp install"
  },
  "dependencies": {},
  "devDependencies": {
    "async": "^1.4.2",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.0",
    "chai": "^3.2.0",
    "coveralls": "^2.11.1",
    "eslint": "^1.3.0",
    "eslint-plugin-react": "^3.3.0",
    "gulp": "^3.9.0",
    "istanbul": "^0.3.2",
    "jsdom": "^3.1.2",
    "mocha": "^2.0.1",
    "mockery": "^1.4.0",
    "pre-commit": "^1.0.7",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "^0.14.0",
    "shelljs": "^0.5.3"
  },
  "pre-commit": [
    "lint",
    "test"
