{
  "name": "fluxible-repo",
  "version": "1.0.0",
  "private": true,
  "description": "Fluxible monorepo",
  "repository": {
    "type": "git",
    "url": "https://github.com/yahoo/fluxible"
  },
  "scripts": {
    "test": "mocha packages/*/tests/unit/ --recursive --compilers js:babel/register --reporter spec",
